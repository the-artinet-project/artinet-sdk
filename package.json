{
  "name": "@artinet/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Agent2Agent (A2A) Protocol",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm test",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "agent2agent",
    "a2a",
    "artinet",
    "ai",
    "agent",
    "sdk",
    "protocol"
  ],
  "author": "Artinet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/artinet/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/artinet/sdk/issues"
  },
  "homepage": "https://github.com/artinet/sdk#readme",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/pino": "^7.0.4",
    "cors": "^2.8.5",
    "eventsource-parser": "^1.1.1",
    "express": "^4.18.2",
    "pino": "^8.15.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.6",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "msw": "^2.7.5",
    "pino-pretty": "^10.2.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
